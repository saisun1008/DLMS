<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from WebServiceControllerService WSDL service -->

  <service name="WebServiceController" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://web.service.dlms"/>
      <parameter name="wsdlServiceElement" value="WebServiceControllerService"/>
      <parameter name="schemaQualified" value="http://web.service.dlms"/>
      <parameter name="wsdlServicePort" value="WebServiceController"/>
      <parameter name="className" value="dlms.service.web.WebServiceControllerSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="WebServiceController"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="printCustomerInfo" qname="operNS:printCustomerInfo" xmlns:operNS="http://web.service.dlms" returnQName="retNS:printCustomerInfoReturn" xmlns:retNS="http://web.service.dlms" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:bank" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="delayPayment" qname="operNS:delayPayment" xmlns:operNS="http://web.service.dlms" returnQName="retNS:delayPaymentReturn" xmlns:retNS="http://web.service.dlms" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:bank" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:loanID" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:currentDueDate" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:newDueDate" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="transferLoan" qname="operNS:transferLoan" xmlns:operNS="http://web.service.dlms" returnQName="retNS:transferLoanReturn" xmlns:retNS="http://web.service.dlms" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:LoanID" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:CurrentBank" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:OtherBank" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="openAccount" qname="operNS:openAccount" xmlns:operNS="http://web.service.dlms" returnQName="retNS:openAccountReturn" xmlns:retNS="http://web.service.dlms" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:bank" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:firstName" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:lastName" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:emailAddress" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:phoneNumber" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:password" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getLoan" qname="operNS:getLoan" xmlns:operNS="http://web.service.dlms" returnQName="retNS:getLoanReturn" xmlns:retNS="http://web.service.dlms" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:bank" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:accountNumber" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:password" xmlns:pns="http://web.service.dlms" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:loanAmount" xmlns:pns="http://web.service.dlms" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="transferLoan getLoan delayPayment printCustomerInfo openAccount"/>

  </service>
</deployment>
